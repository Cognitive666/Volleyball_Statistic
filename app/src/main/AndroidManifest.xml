<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.volleyball_statistic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Volleyball Statistic"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Volleyball_Statistic">
        <activity
            android:name=".TeamCreate"
            android:exported="false" />
        <activity
            android:name=".UdarActivity"
            android:exported="false" />
        <activity
            android:name=".PodachaActivity"
            android:exported="false" />
        <activity
            android:name=".GameMenu"
            android:exported="false" />
        <activity
            android:name=".Menu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Game"
            android:exported="false" />
        <activity
            android:name=".Statistic"
            android:exported="false" />
    </application>

</manifest>